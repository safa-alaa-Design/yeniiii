@model ANAILYAHOME.entityes.FotoEntity
@{
    ViewData["Title"] = "yeniFotograf";
    Layout = "~/Views/Shared/_LayoutAdmen.cshtml";
}

<h1>yeniFotograf</h1>




<form class="form-group" method="post">
    
  
    @Html.LabelFor(x =>x.foto)
    @Html.TextBoxFor(x=>x.foto, new {@class ="form-control"})
    <br />

    @Html.LabelFor(x =>x.UrunId)
    @Html.TextBoxFor(x=>x.UrunId, new {@class ="form-control"})
    <br />


    <button class="btn btn-success">Fotograf Ekle</button>
</form>

