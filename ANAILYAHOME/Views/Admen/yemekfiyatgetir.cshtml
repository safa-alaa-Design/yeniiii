@model ANAILYAHOME.entityes.yemekfiyatler
@{
    ViewData["Title"] = "yemekfiyatgetir";
    Layout = "~/Views/Shared/_LayoutAdmen.cshtml";
}






@using (Html.BeginForm("yemekfiyatguncele", "Admen", FormMethod.Post))
{
    <h2 style="font-weight:700;margin-top:-50px;padding-bottom:15px">Yemek Odasi Fıyat Getir</h2>

    @Html.LabelFor(x =>x.id)
    @Html.TextBoxFor(x=>x.id, new {@class ="form-control"})
    <br />
    @Html.LabelFor(x =>x.sandaliye)
    @Html.TextBoxFor(x=>x.sandaliye, new {@class ="form-control"})
    <br />

    <button class="btn btn-success">Fiyat Ekle</button>
}



