@model ANAILYAHOME.Models.TeslimSuresi
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_LayoutAdmen.cshtml";
}







<h1>Create</h1>
<hr />
<div class="row">
    <div class="col-md-10">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
              <div class="form-group">
                <select asp-for="@Model.ulkeler" class="form-control" asp-items="Html.GetEnumSelectList<ulkeler>()">
                   <option value="0">Ülkeniz seç</option>
                </select>
                 <span asp-validation-for="@Model.ulkeler" class="text-danger"></span>
              </div>
            <br>
            <div class="form-group">
                <label asp-for="teslimsuresi" class="control-label"></label>
                <input asp-for="teslimsuresi" class="form-control" />
                <span asp-validation-for="teslimsuresi" class="text-danger"></span>
            </div>
            <br><br>
            <div class=" col-12 btn-group p-0 m-0">
                <div class="col-6 form-group text-left p-0">
                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>
            </div>




        </form>

    </div>


</div>





@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    @*//لا يتم تحميل الصفحه اذا ما تعبت الانبةتات*@
    <partial name="_ValidationScriptsPartial" />
}
